{
  "id": null,
  "uid": null,
  "title": "FrotaPRO - Observabilidade Básica",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "tags": ["frotapro", "spring-boot", "micrometer"],
  "style": "dark",
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "Uptime (segundos)",
      "id": 1,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "process_uptime_seconds",
          "legendFormat": "uptime"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "CPU do Processo",
      "id": 2,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "process_cpu_usage",
          "legendFormat": "process_cpu"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "CPU do Sistema",
      "id": 3,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "system_cpu_usage",
          "legendFormat": "system_cpu"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Memória Heap Usada",
      "id": 4,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(jvm_memory_used_bytes{area=\"heap\"})",
          "legendFormat": "heap used"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" }
      }
    },
    {
      "type": "timeseries",
      "title": "Memória Non-Heap Usada",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(jvm_memory_used_bytes{area=\"nonheap\"})",
          "legendFormat": "nonheap used"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" }
      }
    },
    {
      "type": "timeseries",
      "title": "Threads da JVM",
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": "$datasource",
      "targets": [
        { "refId": "A", "expr": "jvm_threads_live_threads", "legendFormat": "live" },
        { "refId": "B", "expr": "jvm_threads_daemon_threads", "legendFormat": "daemon" },
        { "refId": "C", "expr": "jvm_threads_peak_threads", "legendFormat": "peak" }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" }
      }
    },
    {
      "type": "timeseries",
      "title": "Pausas de GC (contagem)",
      "id": 7,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "rate(jvm_gc_pause_seconds_count[5m])",
          "legendFormat": "gc pauses /s"
        }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" }
      }
    },
    {
      "type": "timeseries",
      "title": "Taxa de Requisições HTTP (qps)",
      "id": 8,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_requests_seconds_count[1m]))",
          "legendFormat": "req/s"
        }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" }
      }
    },
    {
      "type": "timeseries",
      "title": "Erros HTTP 4xx / 5xx",
      "id": 9,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"4..\"}[5m]))",
          "legendFormat": "4xx"
        },
        {
          "refId": "B",
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m]))",
          "legendFormat": "5xx"
        }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" }
      }
    },
    {
      "type": "timeseries",
      "title": "Latência HTTP p95",
      "id": 10,
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "datasource": "$datasource",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "selected": true,
          "text": "prometheus",
          "value": "prometheus"
        }
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"]
  }
}
